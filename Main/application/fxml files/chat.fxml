<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXListView?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchor" prefHeight="500.0" prefWidth="1000.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Chat">
   <children>
      <Label layoutX="318.0" layoutY="32.0" prefHeight="55.0" prefWidth="127.0" text="Chat">
         <font>
            <Font size="43.0" />
         </font>
      </Label>
      <JFXTextField fx:id="search" layoutX="629.0" layoutY="39.0" promptText="Search Friends" />
      <Label fx:id="name" layoutX="268.0" layoutY="146.0" prefHeight="93.0" prefWidth="561.0">
         <font>
            <Font size="64.0" />
         </font>
      </Label>
      <Button fx:id="cross" layoutX="342.0" layoutY="308.0" mnemonicParsing="false" onAction="#sendmessage" prefHeight="49.0" prefWidth="206.0" text="Send Message">
         <font>
            <Font size="22.0" />
         </font>
      </Button>
      <Button layoutX="688.0" layoutY="84.0" mnemonicParsing="false" onAction="#search" text="search" />
      <JFXListView fx:id="lv" layoutX="863.0" layoutY="122.0" prefHeight="211.0" prefWidth="115.0" />
      <Button fx:id="cross1" layoutX="588.0" layoutY="309.0" mnemonicParsing="false" onAction="#sendrequest" prefHeight="49.0" prefWidth="204.0" text="Send Request">
         <font>
            <Font size="22.0" />
         </font>
      </Button>
      <Button fx:id="friendreq" layoutX="882.0" layoutY="64.0" mnemonicParsing="false" onAction="#friendreq" text="friend requests" />
      <JFXListView fx:id="friends" layoutX="8.0" layoutY="69.0" prefHeight="417.0" prefWidth="206.0" />
      <Button fx:id="yourfriends" layoutX="56.0" layoutY="32.0" mnemonicParsing="false" onAction="#friends" text="Friends">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="cancel" layoutX="908.0" layoutY="14.0" mnemonicParsing="false" onAction="#cross" prefHeight="25.0" prefWidth="66.0" text="X" />
      <Button fx:id="min" layoutX="836.0" layoutY="7.0" mnemonicParsing="false" onAction="#min" prefHeight="25.0" prefWidth="66.0" text="-">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
      <Button fx:id="back" layoutX="14.0" layoutY="39.0" mnemonicParsing="false" onAction="#back" prefHeight="15.0" prefWidth="36.0" text="&lt;" />
   </children>
</AnchorPane>
